{
  "extends": "rackt",
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/wrap-multilines": 2, 
    "indent": 0,
    "no-unused-vars": 0,
    "no-mixed-spaces-and-tabs": 1,
    "no-console": 1
  },
  "plugins": [
    "react"
  ]
}
